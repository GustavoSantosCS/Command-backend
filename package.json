{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node dist/server.js",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 ./dist/server.js",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig-build.json",
    "dev": "tsc -p tsconfig-build.json -w",
    "pretest": "jest --clearCache",
    "test": "jest --passWithNoTests --no-cache",
    "test:unit": "npm test -- --maxWorkers=50% --watch -c jest-unit.config.js",
    "test:integration": "npm test -- --watch --runInBand -c jest-integration.config.js",
    "test:staged": "yarn test -- --maxWorkers=50% --findRelatedTests",
    "test:coveralls": "jest --maxWorkers=50% --silent --coverage && coveralls < coverage/lcov.info",
    "up": "docker-compose up -d",
    "down": "docker-compose down",
    "check": "npm-check -s -u",
    "typeorm": "tsnd ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "aws-sdk": "^2.958.0",
    "bcrypt": "^5.0.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.33",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.12",
    "@types/faker": "^5.5.6",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/multer": "^1.4.5",
    "@types/node": "^15.0.2",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "coveralls": "^3.1.0",
    "eslint": "^7.14.0",
    "eslint-config-standard-with-typescript": "^19.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "faker": "^5.5.3",
    "git-commit-msg-linter": "^3.0.0",
    "husky": "4.2.5",
    "jest": "^26.6.3",
    "lint-staged": "^10.2.13",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "sqlite3": "^5.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.6",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  },
  "engines": {
    "node": "14.x"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
